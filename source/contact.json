{
    "pageTitle":{
    	"class":"pageTitle cf",
        "image":{
            "src"   :"image/icon(mailbird).png"
        },
        "title"     : {
            "text"  :   "お問い合わせフォーム"
        },
        "summary"   : {
            "text"  :   ""
        }
    },
    "contactForm":{
        "action":"postmail.cgi",
        "method": "post",
        "formBody.cf":{
            "name":{
                "formContent":{
                    "text":"氏名"
                },
                "textRed":{
                    "text":"*"
                },
                "formRuby":{
                    "text":"Name"
                }
            },
            "nameTextBox":{
                "textBox1":{
                    "type":"text",
                    "name":"name",
                    "size":"60",
                    "required":""
                }
            },
            "sex":{
                "formContent":{
                    "text":"性別"
                },
                "textRed":{
                    "text":"*"
                },
                "formRuby":{
                    "text":"Sex"
                }
            },
            "sexRadioButton":{
                "sexRadioButtonFemale":{
                    "type": "radio",
                    "name": "sex",
                    "value": "女",
                    "checked": ""
                },
                "sexRadioButtonFemaleLabel":{
                    "text": "女"
                },
                "sexRadioButtonMale":{
                    "type": "radio",
                    "name": "sex",
                    "value": "男"
                },
                "sexRadioButtonMaleLabel":{
                    "text":"男"
                }
            },
            "eMail":{
                "formContent":{
                    "text":"メールアドレス"
                },
                "textRed":{
                    "text":"*"
                },
                "formRuby":{
                    "text":"E-mail"
                }
                
            },
            "eMailTextBox":{
                "textBox2":{
                    "type": "email",
                    "name": "eMail",
                    "size": "60",
                    "required": ""
                }
            },
            "content":{
                "formContent":{
                    "text":"問い合わせ内容"
                },
                "textRed":{
                    "text":"*"
                },
                "formRuby":{
                    "text":"content"
                }
            },
            "contentTextArea":{
                "textArea1":{
                    "name": "content",
                    "cols": "60",
                    "rows": "10",
                    "required": ""
                }
            }
        },
        "submitButton":{
            "type": "submit",
            "text": "送信内容の確認"
        }
    }
}
